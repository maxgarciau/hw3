<h1>My posts in <%=@place["name"] %></h1>


<ul>
<% for post in @posts %>
  <h1><%= post["title"] %></h1>
  <h2><%= post["posted_on"] %></h2>
  <p><%= post["description"] %></p>
<% end %>
</ul>


<p><a href="/posts/new?place_id=<%= @place["id"] %>">New Post</a></p>
<p><a href="/places/">Back to places I've been</a></p>